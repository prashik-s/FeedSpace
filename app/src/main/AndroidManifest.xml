<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feedspace.loginregister">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="FeedSpace"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.feedspace.loginregister.activities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.feedspace.loginregister.activities.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.feedspace.loginregister.activities.UsersListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.feedspace.loginregister.activities.PostFeed"
            android:screenOrientation="portrait" />
    </application>

</manifest>